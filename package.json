{
  "name": "outlook-meeting-analytics-addin",
  "version": "1.0.0",
  "description": "Add-in Outlook on-premise pour l'analytique des r√©unions avec classification par couleur",
  "main": "dist/taskpane/taskpane.js",
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "dev": "webpack serve --mode development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "validate": "office-addin-manifest validate manifest.xml",
    "start": "office-addin-dev-certs install && office-addin-dev-certs verify",
    "sideload": "office-addin-dev-certs verify && echo 'Utilisez Outlook Desktop pour charger le manifest local'",
    "docker:dev": "./scripts/docker-dev.sh",
    "docker:test": "./scripts/docker-test.sh",
    "docker:build": "./scripts/docker-build.sh",
    "docker:down": "docker-compose down || docker compose down",
    "docker:logs": "docker-compose logs -f || docker compose logs -f"
  },
  "keywords": [
    "outlook",
    "add-in",
    "office",
    "analytics",
    "meetings",
    "on-premise"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@microsoft/office-js": "^1.1.85"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-typescript": "^7.23.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "babel-jest": "^29.7.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.53.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "office-addin-dev-certs": "^1.11.3",
    "office-addin-manifest": "^1.12.4",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}

